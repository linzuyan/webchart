<mapper namespace="domain.mapper.KpiIpAndNumMapper">

    <resultMap id="result2" type="KpiIpAndNumDto">
        <result column="kpiip" property="kpiip" jdbcType="VARCHAR"></result>
        <result column="num" property="num" jdbcType="BIGINT"></result>
    </resultMap>

    <sql id="select_sql">
        SELECT kpiip,num
    </sql>

    <!--<select id="queryPageList" resultMap="result">
