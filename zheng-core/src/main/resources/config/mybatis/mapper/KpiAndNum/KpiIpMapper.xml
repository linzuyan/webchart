    <select id="queryPageList" resultMap="result2">
        <include refid="select_sql" />
        FROM mp_kpiip
        WHERE  num>=#{query.nummin} AND  #{query.nummax}>num
        limit #{query.startRow},#{query.pageSize}
    </select>



    <select id="queryCountPage" resultType="java.lang.Integer">
        SELECT COUNT(1) FROM mp_kpiip
    </select>

